{
  "functions": {
    "app/api/upload/route.js": {
      "maxDuration": 30
    },
    "app/api/upload-stream/route.js": {
      "maxDuration": 60
    },
    "app/api/upload-presigned/route.js": {
      "maxDuration": 60
    },
    "app/api/upload-direct/route.js": {
      "maxDuration": 60
    },
    "app/api/upload-complete/route.js": {
      "maxDuration": 30
    },
    "app/api/chunked-upload/route.js": {
      "maxDuration": 60
    },
    "app/api/files/route.js": {
      "maxDuration": 15
    },
    "app/api/download/route.js": {
      "maxDuration": 15
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}